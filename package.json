{
  "name": "books-info",
  "version": "1.0.0",
  "description": "A web app that gets information about books based on Goodreads API",
  "main": "server.js",
  "scripts": {
    "test": "test",
    "start": "set DEBUG=app & nodemon server.js",
    "lint": "eslint server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsukeroman/books-info.git"
  },
  "author": "Roman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tsukeroman/books-info/issues"
  },
  "homepage": "https://github.com/tsukeroman/books-info#readme",
  "dependencies": {
    "body-parser": "1.18.3",
    "bootstrap": "4.1.3",
    "debug": "4.1.0",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "jquery": "3.3.1",
    "mysql": "2.16.0",
    "nodemon": "1.18.4"
  },
  "devDependencies": {
    "eslint": "5.6.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
